 <param name="lz" value="$(arg arglz)"/>
 <node pkg="transmitacr" type="transmitacr_node" name="transmitacr_node" output="screen" >
    <!-- Remap topics from those used in code to those on the ROS network -->
 
 </node>
</launch>
