 <arg name="argy" default="0"/>
 <arg name="argz" default="0.6"/>
 <param name="x" value="$(arg argx)"/>
 <param name="y" value="$(arg argy)"/>
 <param name="z" value="$(arg argz)"/>
 <arg name="arglx" default="0.03"/> 
 <arg name="argly" default="0.03"/>
 <arg name="arglz" default="0.50"/>
